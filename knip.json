{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/main.ts",
    "src/app/app.routes.ts",
    "src/app/app.config.ts"
  ],
  "project": [
    "src/**/*.ts",
    "!src/**/*.spec.ts",
    "!src/**/*.d.ts"
  ],
  "ignore": [
    "worktrees/**/*",
    "coverage/**/*",
    "dist/**/*",
    "node_modules/**/*"
  ],
  "ignoreDependencies": [
    "@angular/animations",
    "@angular/common",
    "@angular/core",
    "@angular/forms",
    "@angular/platform-browser",
    "@angular/router",
    "rxjs",
    "zone.js"
  ]
}